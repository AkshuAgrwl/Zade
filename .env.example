# Do not modify
ENVIRONMENT=

# General
# Port should be same as port exposed by Frontend (NEXTJS_PORT)
SITE_URL=http://localhost:3000

# Frontend
FRONTEND_NEXTJS_PORT=3000

# Postgres DB
POSTGRES_PASSWORD=
POSTGRES_DB=postgres
POSTGRES_PORT=5432

# Docker socket location - this value will differ depending on your OS
DOCKER_SOCKET_LOCATION=//var/run/docker.sock

# Google Cloud Project details
GOOGLE_PROJECT_ID=
GOOGLE_PROJECT_NUMBER=

# Supabase
SUPABASE_JWT_SECRET=
SUPABASE_JWT_EXPIRY=3600
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Supabase Kong API Proxy
SUPABASE_KONG_HTTP_PORT=8442
SUPABASE_KONG_HTTPS_PORT=8443

# Supabase PostgREST API
SUPABASE_PGRST_DB_SCHEMAS=public,storage,graphql_public

# Supabase Studio Dashboard
SUPABASE_STUDIO_DASHBOARD_USERNAME=zade
SUPABASE_STUDIO_DASHBOARD_PASSWORD=
SUPABASE_STUDIO_DEFAULT_ORGANIZATION=Default Organization
SUPABASE_STUDIO_DEFAULT_PROJECT=Default Project
# https://github.com/supabase/supabase/pull/19237
SUPABASE_STUDIO_PORT=
# replace if you intend to use Studio outside of localhost
SUPABASE_STUDIO_PUBLIC_URL=

# Supabase Logflare Logs
SUPABASE_LOGFLARE_LOGGER_BACKEND_API_KEY=your-super-secret-and-long-logflare-key
# Change vector.toml sinks to reflect this change
SUPABASE_LOGFLARE_API_KEY=your-super-secret-and-long-logflare-key

# Supabase Functions
# NOTE: VERIFY_JWT applies to all functions. Per-function VERIFY_JWT is not supported yet.
SUPABASE_FUNCTIONS_VERIFY_JWT=false

# Supabase Auth
SUPABASE_ADDITIONAL_REDIRECT_URLS=
# Port should be same as the port exposed by API handler (SUPABASE_KONG_HTTP_PORT)
SUPABASE_API_EXTERNAL_URL=http://localhost:8442
SUPABASE_DISABLE_SIGNUP=false
# Mailer
SUPABASE_MAILER_URLPATHS_CONFIRMATION="/auth/v1/verify"
SUPABASE_MAILER_URLPATHS_INVITE="/auth/v1/verify"
SUPABASE_MAILER_URLPATHS_RECOVERY="/auth/v1/verify"
SUPABASE_MAILER_URLPATHS_EMAIL_CHANGE="/auth/v1/verify"
# Email
SUPABASE_ENABLE_EMAIL_SIGNUP=true
SUPABASE_ENABLE_EMAIL_AUTOCONFIRM=false
SUPABASE_SMTP_ADMIN_EMAIL=admin@zade.com
SUPABASE_SMTP_HOST=supa-mail
SUPABASE_SMTP_PORT=2500
SUPABASE_SMTP_USER=zade_mail_user
SUPABASE_SMTP_PASS=
SUPABASE_SMTP_SENDER_NAME=Zade Admin
SUPABASE_ENABLE_ANONYMOUS_USERS=false
## Phone auth
SUPABASE_AUTH_ENABLE_PHONE_SIGNUP=true
SUPABASE_AUTH_ENABLE_PHONE_AUTOCONFIRM=true

# Supabase Imgproxy
# Enable webp support
SUPABASE_IMGPROXY_ENABLE_WEBP_DETECTION=true

# Supabase Storage S3 backend
# Only in effect when the project is run with S3 backend enabled
SUPABASE_STORAGE_S3_GLOBAL_S3_BUCKET=stub
SUPABASE_STORAGE_S3_REGION=stub
